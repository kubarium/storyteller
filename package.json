{
  "name": "storyteller",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "stylize": "cd public/styles/ && cross-var node compiler.js --env=p --file=$STORYTELLER && yarn deploy",
    "deploy": "vue-cli-service build && gh-pages -d dist"
  },
  "homepage": "https://kubarium.github.io/storyteller/",
  "dependencies": {
    "axios": "^0.21.1",
    "cloudinary-core": "^2.6.3",
    "concurrently": "^4.1.0",
    "cross-var": "^1.1.0",
    "dropbox": "^4.0.15",
    "gh-pages": "^2.0.1",
    "markdown-it": "^8.4.2",
    "vue": "^2.5.22",
    "vue-codemirror": "^4.0.6",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  }
